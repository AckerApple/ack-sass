{
  "name": "ack-sass",
  "version": "1.0.14",
  "description": "node-sass implementation that greatly reduces configuration setup and also comes with css file importer",
  "main": "index.js",
  "scripts": {
    "test": "node test/scss.js",
    "build": "node cli-test test/styles.scss test/compiled-tests/build-css-styles.css --production",
    "build:path": "node cli-test test test/compiled-tests --directory --production",
    "watch": "node cli-test test/styles.scss test/compiled-tests/build-css-styles.css --watch",
    "watch:path": "node cli-test test test/compiled-tests --directory --watch"
  },
  "bin": {
    "ack-sass": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AckerApple/ack-sass.git"
  },
  "keywords": [
    "sass",
    "scss"
  ],
  "author": "Acker Apple",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AckerApple/ack-sass/issues"
  },
  "homepage": "https://github.com/AckerApple/ack-sass#readme",
  "dependencies": {
    "ack-path": "^1.5.0",
    "node-sass": "^3.13.0",
    "node-sass-css-importer": "0.0.3",
    "watch": "^1.0.2"
  }
}
